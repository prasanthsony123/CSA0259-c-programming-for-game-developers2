#include <stdio.h>
#include <string.h>

int main()
{
    char str[100];
    int sum = 0, len, i;

    printf("Enter a string: ");
    scanf("%s", str);

    len = strlen(str);

    for(i = 0; i < len; i++)
    {
        sum += str[i];
    }

    printf("Sum of ASCII values of all characters in the string: %d", sum);

    return 0;
}
